<table class="table table-bordered mt-5"
       data-toggle="table"
       data-sort-name="serial"
       data-sort-order="asc">

    <thead class="thead-dark">
    <tr>
        <th scope="col" class="align-middle" data-sortable="true">Type</th>
        <th scope="col" class="align-middle" data-sortable="true" data-field="serial">Serial â„–</th>
        <th scope="col" class="align-middle">Date</th>
        <th scope="col" class="align-middle">Key</th>
    </tr>
    </thead>

    <tbody>
    <% devices.forEach((device) => { %>
        <tr>
            <td scope="row" class="align-middle"><%= device.type.name %></td>
            <td scope="row" class="align-middle"><%= device.serial %></td>
            <td class="align-middle"><%= device.year %>-<%= device.month %></td>
            <td class="align-middle"><%= device.hash %></td>
        </tr>
    <% }) %>
    </tbody>

</table>